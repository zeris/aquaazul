<% for(const usuario of resultadosBusqueda) { %>
    <div class="row all-padding-10px">
        <div class="col-lg-7 col-12  height-margin-2">
            <h4> <%= usuario.NOMBRE + ' ' + usuario.APELLIDO_P + ' ' + usuario.APELLIDO_M %> </h4>
        </div>
        <div class="col-lg-2 col-12 height-margin-2">
            <form action="/administrador/usuarios/actualizar/<%= usuario.ID_USUARIO %>" method="GET">
                <input class="btn btn-success col-12" type="submit" value="Actualizar"/>
            </form>
        </div>
        <div class="col-lg-2 col-12 height-margin-2">
            <form action="/administrador/usuarios/eliminar/<%= usuario.ID_USUARIO %>" method="GET">
                <input class="btn btn-danger col-12" type="submit" value="Eliminar"/>
            </form>
        </div>     
    </div>
<% } %>