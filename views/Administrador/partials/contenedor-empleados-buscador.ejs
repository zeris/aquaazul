<% for(const empleado of resultadosBusqueda) { %>
    <div class="row">
        <div class="col-12">
            <div class="col-8">
                <h4> <%= empleado.NOMBRE + ' ' + empleado.APELLIDO_P + ' ' + empleado.APELLIDO_M %> </h4>
            </div>
            <div class="col-2">
                <a href="/administrador/empleados/actualizar/<%= empleado.ID_USUARIO %>">Actualizar</a>
            </div>
            <div class="col-2">
                <a href="/administrador/empleados/eliminar/<%= empleado.ID_USUARIO %>">Eliminar</a>    
            </div>     
        </div>
    </div>
<% } %>